<?xml version="1.0"?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <sites>
      <site name="varsity" patch:after="site[@name='modules_website']"
            targetHostName="sc.local"
            database="web"
            virtualFolder="/"
            physicalFolder="/"
            rootPath="/sitecore/content/varsity"
            startItem="/Home"
            dictionaryAutoCreate="false"
            domain="extranet"
            allowDebug="true"
            cacheHtml="true"
            htmlCacheSize="50MB"
            registryCacheSize="0"
            viewStateCacheSize="0"
            xslCacheSize="25MB"
            filteredItemsCacheSize="10MB"
            enablePreview="true"
            enableWebEdit="true"
            enableDebugger="true"
            disableClientData="false"
            cacheRenderingParameters="true"
            renderingParametersCacheSize="10MB"
            enableItemLanguageFallback="true" />
    </sites>
    <settings>
      <setting name="Preview.DefaultSite">
        <patch:attribute name="value" value="varsity" />
      </setting>
    </settings>
  </sitecore>
</configuration>